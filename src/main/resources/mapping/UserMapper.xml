<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jnshu.dao.UserDao">
    <select id="selectAllUser" resultType="com.jnshu.model.User">
        SELECT * FROM task4
    </select>
    <select id="getUserIdList" resultType="long">
        SELECT id FROM task4
    </select>
    <select id="getId" resultType="com.jnshu.model.User">
        SELECT * FROM task4 WHERE id=#{id}
    </select>
</mapper>